{
	"name": "scutum_admin_vuejs",
	"version": "2.5.0",
	"description": "Scutum Admin Template (Vue.js version)",
	"author": "tzd",
	"private": true,
	"scripts": {
		"dev": "nuxt",
		"build": "nuxt build",
		"start": "nuxt start",
		"generate": "nuxt generate",
		"sc-dev": "nuxt --port 3103",
		"sc-install": "yarn install --ignore-engines",
		"sc-install:npm": "npm run install",
		"sc-setup": "yarn sc-install && yarn sc-uikit",
		"sc-setup:npm": "npm run sc-install && npm run sc-uikit",
		"sc-update": "yarn sc-cleanup && ncu -u",
		"sc-update:npm": "npm run sc-cleanup && ncu -u -i",
		"sc-cleanup": "rm -rf node_modules && rm -rf yarn.lock",
		"sc-uikit": "node ./build/custom-uikit.js",
		"sc-mdi": "node ./build/copy-mdi.js"
	},
	"dependencies": {
		"@braid/vue-formulate": "^2.5.2",
		"@mdi/font": "^4.9.95",
		"@nuxtjs/axios": "^5.13.0",
		"@nuxtjs/pwa": "^3.3.5",
		"bootstrap": "^4.6.0",
		"bootstrap-vue": "^2.21.2",
		"core-js": "^3.15.1",
		"crypto-js": "^4.1.1",
		"inputmask": "^5.0.6",
		"node-sass": "^6.0.1",
		"nuxt": "^2.14.12",
		"pretty-checkbox-vue": "^1.1.9",
		"sass-loader": "^10.1.1",
		"uikit": "^3.6.15",
		"velocity-animate": "^1.5.2",
		"vue": "2.6.14",
		"vue-tel-input": "^5.5.0",
		"vue2-touch-events": "^3.1.1",
		"vuelidate": "^0.7.6",
		"select2": "^4.0.13",
		"jquery": "^3.5.1",
		"vue-template-compiler": "^2.6.12"
	},
	"devDependencies": {
		"babel-eslint": "^10.1.0",
		"babel-polyfill": "^6.26.0",
		"eslint": "^7.19.0",
		"eslint-config-prettier": "^7.2.0",
		"eslint-loader": "^4.0.2",
		"eslint-plugin-prettier": "3.3.1",
		"eslint-plugin-vue": "^7.5.0"
	}
}
